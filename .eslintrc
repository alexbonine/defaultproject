{
  "parserOptions": {
    "ecmaVersion": 6,
    "souceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "plugins": ["react"],
  "extends": "airbnb",
  "rules": {
    jsx-quotes: ["warn", "prefer-single"],
    "react/react-in-jsx-scope": "off"
  }
}